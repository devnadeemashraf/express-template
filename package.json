{
  "name": "express-template",
  "version": "1.0.0",
  "description": "ðŸš€Production-ready Express TypeScript backend template",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node ./dist/server.js",
    "dev": "tsx -r tsconfig-paths/register src/server.ts",
    "build:docs": "swagger-jsdoc -d src/config/swagger.ts -o openapi.yaml",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:perf": "jest --testNamePattern='Performance Tests'",
    "test:edge": "jest --testNamePattern='Edge Cases'"
  },
  "keywords": [
    "express",
    "typescript",
    "backend",
    "template",
    "rest-api"
  ],
  "author": "Nadeem Ashraf dev.nadeemashraf06@gmail.com",
  "license": "ISC",
  "engines": {
    "node": ">=23.7.0",
    "npm": ">=11.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devnadeemashraf/express-template.git"
  },
  "bugs": {
    "url": "https://github.com/devnadeemashraf/express-template/issues"
  },
  "homepage": "https://github.com/devnadeemashraf/express-template#readme",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.13.4",
    "@types/swagger-ui-express": "^4.1.7",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "@prisma/client": "6.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-openapi-validator": "^5.4.3",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "module-alias": "^2.2.3",
    "prisma": "^6.4.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
